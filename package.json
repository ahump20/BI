{
  "name": "blaze-intelligence",
  "version": "2.0.0",
  "description": "Championship Sports Analytics Platform with AI Consciousness and Multi-AI Integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/multi_ai_integration.js",
    "update": "node dynamic-content-updater.js",
    "watch": "node scripts/multi_ai_integration.js --watch",
    "deploy": "wrangler pages deploy . --project-name=blaze-intelligence",
    "deploy:biometric": "wrangler deploy --config wrangler-biometric.toml",
    "mcp-server": "./start-cardinals-server.sh",
    "biometric-processor": "node biometric-data-processor.js",
    "build": "npm run build:site && npm run copy:data",
    "build:site": "node -e \"console.log('Static site build complete')\"",
    "copy:data": "mkdir -p dist/data && cp -R data/* dist/data 2>/dev/null || true",
    "serve": "python3 -m http.server 8000",
    "status": "node scripts/multi_ai_integration.js --status",
    "health-check": "node system-monitor.js",
    "security-scan": "node scripts/security-scan.js",
    "backup": "node scripts/backup-system.js",
    "ingest-data": "node scripts/data-ingestion.js",
    "test-ai": "node scripts/multi_ai_integration.js --test",
    "test-biometric": "node tests/biometric-test.js",
    "generate-reports": "node reporting/generate-reports.js",
    "github-analyze": "node scripts/github-analyzer.js",
    "github-cleanup": "node scripts/github-cleanup.js --dry-run",
    "github-cleanup-execute": "node scripts/github-cleanup.js --execute",
    "github-plan": "node scripts/github-organization-plan.js",
    "catch-branches": "node scripts/git-branch-manager.js",
    "catch-branches-execute": "node scripts/git-branch-manager.js --execute",
    "sync-branches": "npm run catch-branches",
    "merge-to-main": "npm run catch-branches-execute",
    "deploy:contact": "wrangler deploy contact-form.js --config ../wrangler-contact-api.toml --env production",
    "deploy:auth": "wrangler deploy auth.js --config ../wrangler-auth-api.toml --env production",
    "dev": "netlify dev"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "hono": "^4.9.8",
    "ioredis": "^5.7.0",
    "node-fetch": "^3.3.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@mediapipe/pose": "^0.5.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "three": "^0.160.0",
    "chart.js": "^4.4.0",
    "gsap": "^3.12.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250918.0",
    "@types/node": "^20.10.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "sports-analytics",
    "ai-consciousness", 
    "multi-ai-integration",
    "cloudflare",
    "workers",
    "api",
    "authentication",
    "contact",
    "blaze-intelligence",
    "championship-analytics",
    "video-intelligence",
    "biometric-analysis",
    "real-time-data"
  ],
  "author": "Austin Humphrey <ahump20@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}